{
    "type": "dqn_agent",
    "max_episodes": 15000,
    "max_episode_timesteps": 10000,
    "states_preprocessing_spec": [
        {
            "type": "image_resize",
            "width": 84,
            "height": 84
        },
        {
            "type": "grayscale"
        },
        {
            "type": "divide",
            "scale": 255
        },
        {
            "type": "sequence",
            "length": 4
        }
    ],
    "explorations_spec": {
        "type": "epsilon_anneal",
        "initial_epsilon": 1.0,
        "final_epsilon": 0.1,
        "timesteps": 1e6
    },
    "batch_size": 32,
    "memory": {
        "type": "replay",
        "random_sampling": true,
        "capacity": 100000
    },
    "update_frequency": 4,
    "first_update": 50000,
    "repeat_update": 1,
    "target_sync_frequency": 10000,
    "target_update_weight": 1.0,
    "discount": 0.99,
    "optimizer": {
        "type": "adam",
        "learning_rate": 0.00025
    },
    "huber_loss": 1.0,
    "network": [
        {
            "type": "conv2d",
            "size": 32,
            "window": 8,
            "stride": 4,
            "bias": true
        },
        {
            "type": "conv2d",
            "size": 64,
            "window": 4,
            "stride": 2,
            "bias": true
        },
        {
            "type": "conv2d",
            "size": 64,
            "window": 3,
            "stride": 1,
            "bias": true
        },
        {
            "type": "flatten"
        },
        {
            "type": "dense",
            "size": 256,
            "bias": true
        }
    ]
}
